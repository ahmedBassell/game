{
  "extends": "eslint:recommended",
   "env": {
      "es6": true,
      "node": true
  },
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "warn"
  },
  "globals": {
    "require": 1
  }
}
